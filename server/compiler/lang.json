[
    {
        "lang"              : "python",
        "extension"         : "py",
        "commands"          : [
            "python {filename}.py"
        ]
    },
    {
        "lang"              : "javascript",
        "extension"         : "js",
        "commands"          : [
            "node {filename}.js"
        ]
    },
    {
        "lang"              : "c",
        "extension"         : "c",
        "commands"          : [ 
            "gcc {filename}.c -o {filename}",
            "./{filename}"
        ]
    },
    {
        "lang"              : "c++",
        "extension"         : "cpp",
        "commands"          : [ 
            "gcc {filename}.c -o {filename}",
            "./{filename}"
        ]
    },
    {
        "lang"              : "java",
        "extension"         : "java",
        "commands"          : [ 
            "javac {filename}.java",
            "java {filename}"
        ]
    }
]